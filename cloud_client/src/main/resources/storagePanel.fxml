<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="center" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.2-internal"
      fx:controller="krisnovitskaya.cloudstorage.clientside.StoragePanelController">

    <HBox >
        <Button text="Вверх" fx:id="storageBtnUp" onAction="#storageDirUp"/>
        <TextField fx:id="storagePathField" editable="false" HBox.hgrow="ALWAYS" />
    </HBox>
    <TableView fx:id="storageFilesTable" VBox.vgrow="ALWAYS" />

    <properties>
        <ctrl><fx:reference source="controller"/></ctrl>
    </properties>
</VBox>
